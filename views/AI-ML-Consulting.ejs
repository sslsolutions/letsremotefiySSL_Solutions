    <style>
      .mySlides {
        display: none;
      }
      img {
        vertical-align: middle;
      }

      /* Slideshow container */
      .slideshow-container {
        max-width: 90%;
        position: relative;
        margin: 0 auto;
      }

      /* Caption text */
      .text {
        color: #f2f2f2;
        font-size: 15px;
        padding: 8px 12px;
        position: absolute;
        bottom: 8px;
        width: 100%;
        text-align: center;
      }

      /* Number text (1/3 etc) */
      .numbertext {
        color: #f2f2f2;
        font-size: 12px;
        padding: 8px 12px;
        position: absolute;
        top: 0;
      }

      /* The dots/bullets/indicators */
      .dot {
        height: 15px;
        width: 15px;
        margin: 0 2px;
        background-color: #bbb;
        border-radius: 50%;
        display: inline-block;
        transition: background-color 0.6s ease;
      }

      .dotactive {
        background-color: #717171;
      }

      /* Fading animation */
      .fade {
        animation-name: fade;
        animation-duration: 1.5s;
      }

      @keyframes fade {
        from {
          opacity: 0.4;
        }
        to {
          opacity: 1;
        }
      }

      /* On smaller screens, decrease text size */
      @media only screen and (max-width: 300px) {
        .text {
          font-size: 11px;
        }
      }

      .container {
        display: flex;
        flex-direction: column;
        max-width: 1200px;
        margin: 0 auto;
        margin-top: 10rem;
      }

      .nav {
        display: flex;
        justify-content: space-around;
        padding: 10px;
      }

      .service-link {
        color: gray;
        font-weight: 700;
        text-decoration: none;
        cursor: pointer;
      }

      .service-link.serv-active {
        text-decoration: underline;
        color: black;
        text-decoration-color: #AF2022;
        text-decoration-thickness: 5px;
      }

      .content-container {
        display: flex;
        flex-wrap: wrap;
        padding: 1rem;
      }

      .left-container,
      .right-container {
        flex: 1;
        padding: 20px;
      }

      .left-container {
        background-color: #eee;
      }

      .right-container {
        background-color: #f5f5f5;
      }

      img {
        max-width: 100%;
        height: auto;
      }
      #contentTitle {
        font-size: 3.75rem;
      }
      #contentParagraph {
        font-size: 1.25rem;
      }
      @media screen and (max-width: 850px) {
          .container .nav  a{
              margin-left: 1.25rem;
              margin-bottom: 0.5rem;
          }
        }
       @media screen and (max-width: 1116px) {
          #WORKH-SOFT{
              margin-top: 55px !important;
          }
        }
    </style>

