  <script>
    // script.js
    const slider = document.querySelector('.slider');
    const images = [
      '/images/livewire.svg',
      '/images/smarter-sorting.svg',
      '/images/eduleaf-1.svg',
      '/images/zooppa.svg',
      '/images/kudo.svg',
      '/images/showgizmo.svg',

    ];
    const slideInterval = 1000;
    let currentIndex = 0;

    function slide() {

      const currentItem = document.querySelector('.slider-item');
      currentItem.remove();


      const nextIndex = currentIndex % images.length;
      const nextImage = document.createElement('div');
      nextImage.className = 'slider-item';
      nextImage.innerHTML = `<img src="${images[nextIndex]}" alt="Image ${nextIndex + 1}">`;
      slider.appendChild(nextImage);

      currentIndex++;

      setTimeout(slide, slideInterval);
    }

    slide();

