    <script>
      let slideIndex = 0;
      showSlides();

      function showSlides() {
        let i;
        let slides = document.getElementsByClassName("mySlides");
        let dots = document.getElementsByClassName("dot");

        // Hide all slides and set opacity to 0
        for (i = 0; i < slides.length; i++) {
          slides[i].style.opacity = 0;
          slides[i].style.display = "none";
        }

        // Increment slideIndex
        slideIndex++;

        // Reset slideIndex if it exceeds the number of slides
        if (slideIndex > slides.length) {
          slideIndex = 1;
        }

        // Remove "dotactive" class from all dots
        for (i = 0; i < dots.length; i++) {
          dots[i].className = dots[i].className.replace(" dotactive", "");
        }

        // Display the current slide and gradually fade it in
        slides[slideIndex - 1].style.display = "block";
        fadeIn(slides[slideIndex - 1]);

        // Add "active" class to the corresponding dot
        dots[slideIndex - 1].className += " dotactive";

        // Call showSlides every 3 seconds
        setTimeout(showSlides, 3000);
      }

      // Function to gradually fade in an element
      function fadeIn(element) {
        let opacity = 0;
        const fadeInInterval = setInterval(function () {
          if (opacity < 1) {
            opacity += 0.05;
            element.style.opacity = opacity;
          } else {
            clearInterval(fadeInInterval);
          }
        }, 50);
      }

      function showContent(event, linkId) {
        event.preventDefault();

        const links = document.querySelectorAll(".service-link");
        const contentTitle = document.getElementById("contentTitle");
        const contentParagraph = document.getElementById("contentParagraph");

        // Remove "active" class from all links
        links.forEach((link) => link.classList.remove("serv-active"));

        // Set the clicked link as active
        const clickedLink = document.querySelector(`[onclick*="${linkId}"]`);
        clickedLink.classList.add("<serv-active></serv-active>");

        // Update content based on the clicked link
        switch (linkId) {
          case "link1":
            contentTitle.textContent = "Cloud Strategy Consulting";
            contentParagraph.textContent =
              "We assist in leading new enterprise-level cloud infrastructure by delivering the groundwork for a smooth transition to the cloud with a comprehensive cloud adoption strategy that addresses fundamental technological changes.";
            break;
          case "link2":
            contentTitle.textContent = "Infrastructure as a Service (IaaS)";
            contentParagraph.textContent =
              "We intend to migrate your organization’s infrastructure to an IaaS solution to assist you in reducing the maintenance of on-premises data centers, saving money on hardware costs, and attaining real-time business insights.";
            break;
          case "link3":
            contentTitle.textContent = "Software as a Service (SaaS)";
            contentParagraph.textContent =
              "Software as a Service delivers software applications over the internet, on-demand, and on a subscription basis. With SaaS, the cloud delivers the host and manages the software application and underlying infrastructure to handle maintenance.";
            break;
          case "link4":
            contentTitle.textContent = "Platform as a Service (PaaS)";
            contentParagraph.textContent =
              "We offer a platform as a service to cloud computing that supplies on-demand for developing, testing, delivering, and managing software applications. PaaS makes things easier for developers to create web or mobile apps.";
            break;
          case "link5":
            contentTitle.textContent = "Test and Build Applications";
            contentParagraph.textContent =
              "We reduce application development costs and time through the use of cloud infrastructure that is easily scaled up or down.";
            break;
          case "link6":
            contentTitle.textContent = "Serverless Computing";
            contentParagraph.textContent =
              "We overlap with PaaS, serverless computing that focuses on building app functionality without offering time continually to manage the server management for you. Our Serverless architectures are highly scalable and event-driven.";
