    <script>
        document.getElementById("talentCreateAccount").addEventListener("click", function () {
            sendRoleToBackend("Buyer"); // Send "Buyer" as the selected role
        });

        document.getElementById("hireTalentCreateAccount").addEventListener("click", function () {
            sendRoleToBackend("Seller"); // Send "Seller" as the selected role
        });

        function sendRoleToBackend(selectedRole) {
            // Send a POST request to the backend with the selected role
            fetch('/signup', {
                method: 'POST',
                headers: {
                    'Content-Type': 'application/json',
                },
                body: JSON.stringify({ role: selectedRole }),
            })
                .then(response => {
                    if (response.ok) {
                        console.log('Selected role sent to the backend successfully.');
                    } else {
                        console.error('Failed to send the selected role to the backend.');
                    }
                })
                .catch(error => {
                    console.error('Error:', error);
                });
        }
    </script>
