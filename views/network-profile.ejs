    <script>
            function updateLiveTime() {
            const now = new Date();
            const hours = now.getHours();
            const minutes = now.getMinutes();
            const ampm = hours >= 12 ? 'PM' : 'AM';
            const formattedTime = `${hours % 12 || 12}:${String(minutes).padStart(2, '0')} ${ampm}`;
            
            // Update the content of the 'live-time' div
            const liveTime = document.getElementById('clock');
            liveTime.textContent = `PK - ${formattedTime}`;
        }

        setInterval(updateLiveTime, 1000);
        updateLiveTime(); // Initial call
    </script>
