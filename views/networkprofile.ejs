    <script>

        // Get all the elements with the "seeking-hover" class
        const seekingBoxes = document.querySelectorAll('.seeking-hover');

        // Add a click event listener to each box
        seekingBoxes.forEach(box => {
            box.addEventListener('click', () => {
                // Remove the "active" class from all boxes
                seekingBoxes.forEach(box => {
                    box.classList.remove('active');
                });

                // Add the "active" class to the clicked box
                box.classList.add('active');
            });
        });


        const endDateInput = document.getElementById('end-date');
        const currentlyWorkingCheckbox = document.getElementById('currently-working');
        const label_date = document.getElementById('label_end');

        currentlyWorkingCheckbox.addEventListener('change', function () {
            if (currentlyWorkingCheckbox.checked) {
                endDateInput.style.display = 'none';
                label_date.style.display = 'none'
            } else {
                endDateInput.style.display = 'block';
                label_date.style.display = 'block';
            }
        });

    </script>

